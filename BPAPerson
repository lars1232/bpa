//Nicholas Aparicio
//Period 1

public class BPAPerson
{
	private char typeParticipant;
	private String districtName, street, city, phone, participantName, individualEvent, teamEvent, generalEvent;
	private double riverWalkA, riverWalkB, fiestaTexas, torchAwardLuncheon, totalRegistration;

	BPAPerson()			                         	// Constructor (default)
	{
		districtName = " ";
		street = " ";
		city = " ";
		phone = " ";
		participantName = " ";
		typeParticipant =' ';
		individualEvent = " ";
		teamEvent = " ";
		generalEvent = " ";
		riverWalkA = 0;
		riverWalkB = 0;
		fiestaTexas = 0;
		torchAwardLuncheon = 0;
		totalRegistration = 0;
	}
	BPAPerson(String dn, String str, String cit, String p, String pn, char tp, String ie, String te, String ge, double a, double b, double ft, double tal, double tr)                 //Constructor
	{
		districtName = dn;
		street = str;
		city = cit;
		phone = p;
		participantName = pn;
		typeParticipant = tp;
		individualEvent = ie;
		teamEvent = te;
		generalEvent = ge;
		riverWalkA = a;
		riverWalkB = b;
		fiestaTexas = ft;
		torchAwardLuncheon = tal;
		totalRegistration = tr;
	}

public String getDistrictName()           {return districtName;}
public String getStreet()                 {return street;}
public String getCity()                   {return city;}
public String getPhone()                  {return phone;}
public String getParticipantName()        {return participantName;}
public char getTypeParticipant()		  {return typeParticipant;}
public String getIndividualEvent()        {return individualEvent;}
public String getTeamEvent()              {return teamEvent;}
public String getGeneralEvent()           {return generalEvent;}
public double getRiverWalkA()             {return riverWalkA;}
public double getRiverWalkB()             {return riverWalkB;}
public double getFiestaTexas()            {return fiestaTexas;}
public double getTorchAwardLuncheon()     {return torchAwardLuncheon;}
public double getTotalRegistration()      {return totalRegistration;}

public void setDistrictName(String dn)         {dn = districtName;}
public void setStreet(String str)                {str = street;}
public void setCity(String cit)                  {cit = city;}
public void setPhone(String p)                 {p = phone;}
public void setParticipantName(String pn)      {pn = participantName;}
public void setTypeParticipant(char tp)      {tp = typeParticipant;}
public void setIndividualEvent(String ie)      {ie = individualEvent;}
public void setTeamEvent(String te)            {te = teamEvent;}
public void setGeneralEvent(String ge)         {ge = generalEvent;}
public void setRiverWalkA(double a)            {a = riverWalkA;}
public void setRiverWalkB(double b)            {b = riverWalkB;}
public void setFiestaTexas(double f)           {f = fiestaTexas;}
public void setTorchAwardLuncheon(double tal)  {tal = torchAwardLuncheon;}
public void setTotalRegistration(double tr)    {tr = totalRegistration;}




}
